local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()
local SaveManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/SaveManager.lua"))()
local InterfaceManager = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/Fluent/master/Addons/InterfaceManager.lua"))()

local Window = Fluent:CreateWindow({
    Title = "Update Scripts",
    SubTitle = "After 7 days this script will disappear!",
    TabWidth = 160,
    Size = UDim2.fromOffset(580, 460),
    Acrylic = true, -- The blur may be detectable, setting this to false disables blur entirely
    Theme = "Darker",
    MinimizeKey = Enum.KeyCode.LeftControl -- Used when theres no MinimizeKeybind
})

--Fluent provides Lucide Icons https://lucide.dev/icons/ for the tabs, icons are optional
local Tabs = {
    Dead = Window:AddTab({ Title = "Deadly Delivery", Icon = "building" }),
    am = Window:AddTab({ Title = "After 3am", Icon = "activity" }),
	Settings = Window:AddTab({ Title = "Settings", Icon = "settings" })
}

local Options = Fluent.Options

do end
    Fluent:Notify({
        Title = "Notification Alert",
        Content = "Message From IOER HUB",
        SubContent = "Thank You For Use This Script", -- Optional
        Duration = 5 -- Set to nil to make the notification not disappear
    })



    Tabs.Dead:AddParagraph({
        Title = "Information",
        Content = "Use an Executor That Has 99-100% Unc And Sunc For A Better Experience"
    })

  Tabs.am:AddParagraph({
        Title = "Information",
        Content = "Use an Executor That Has 99-100% Unc And Sunc For A Better Experience"
    })


    Tabs.Dead:AddButton({
        Title = "Hysterical",
        Description = "Work All Executor",
        Callback = function()
            Window:Dialog({
                Title = "Information",
                Content = "Please Wait Until Update!",
                Buttons = {loadstring(game:HttpGet("https://raw.githubusercontent.com/Hysterically-C/DeadlyDeliver/refs/heads/main/Hysterical"))()
                    {
                        Title = "Confirm",
                        Callback = function(v)
                            print("Next Update,Plase Wait")
                        end
                    },
                    {
                        Title = "Cancel",
                        Callback = function(v)
                            print("Cancelled,Are You Sure?")
                        end
                    }
                }
            })
        end
    })

    Tabs.Dead:AddButton({
        Title = "Vgxmod Hub",
        Description = "OVERPOWER SCRIPT",
        Callback = function()
            Window:Dialog({
                Title = "Information",
                Content = "Please Wait Until Update!",
                Buttons = {loadstring(game:HttpGet("https://raw.githubusercontent.com/VGXMODPLAYER68/Vgxmod-Hub/refs/heads/main/Deadly%20delivery.lua"))()
                    {
                        Title = "Confirm",
                        Callback = function(v)
                            print("Next Update,Plase Wait")
                        end
                    },
                    {
                        Title = "Cancel",
                        Callback = function(v)
                            print("Cancelled,Are You Sure?")
                        end
                    }
                }
            })
        end
    })

    Tabs.am:AddButton({
        Title = "No Keyless",
        Description = "Infinite Ammo, Fullbright, Infinite Full And More",
        Callback = function()
            Window:Dialog({
                Title = "Information",
                Content = "Please Wait Until Update!",
                Buttons = {loadstring(game:HttpGet("https://raw.githubusercontent.com/kaghnoa/Scripts/refs/heads/main/Obf_ultrabyxosssjk.after3am"))()
                    {
                        Title = "Confirm",
                        Callback = function(v)
                            print("Next Update,Plase Wait")
                        end
                    },
                    {
                        Title = "Cancel",
                        Callback = function(v)
                            print("Cancelled,Are You Sure?")
                        end
                    }
                }
            })
        end
    })


-- SaveManager (Allows you to have a configuration system)end
-- InterfaceManager (Allows you to have a interface managment system)

-- Hand the library over to our managers
SaveManager:SetLibrary(Fluent)
InterfaceManager:SetLibrary(Fluent)

-- Ignore keys that are used by ThemeManager.
-- (we dont want configs to save themes, do we?)
SaveManager:IgnoreThemeSettings()

-- You can add indexes of elements the save manager should ignore
SaveManager:SetIgnoreIndexes({})

-- use case for doing it this way:
-- a script hub could have themes in a global folder
-- and game configs in a separate folder per game
InterfaceManager:SetFolder("FluentScriptHub")
SaveManager:SetFolder("FluentScriptHub/specific-game")

InterfaceManager:BuildInterfaceSection(Tabs.Settings)
SaveManager:BuildConfigSection(Tabs.Settings)


Window:SelectTab(1)

Fluent:Notify({
    Title = "Information",
    Content = "The script has been loaded.",
    Duration = 8
})

-- You can use the SaveManager:LoadAutoloadConfig() to load a config
-- which has been marked to be one that auto loads!
SaveManager:LoadAutoloadConfig()

print("Update Scripts loaded Successfully, Thanks for Using this script!")
print("Created By IOER Hub, pastebin.com/u/ty4rds")
